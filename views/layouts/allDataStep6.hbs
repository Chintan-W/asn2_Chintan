<!DOCTYPE html>
<html>
<head>
    <title>{{title}}</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
    <h1>{{title}}</h1>
    <form action="/allData/Step6" method="get">
        <label for="search">Search by InvoiceNo or Manufacturer:</label>
        <input type="text" name="search" id="search" placeholder="Enter search term">
        <button type="submit">Search</button>
    </form>
    <table>
        <thead>
            <tr>
                <th>InvoiceNo</th>
                <th>Manufacturer</th>
                <th>Class</th>
                <th>Sales (thousands)</th>
                <th>Year Resale Value</th>
                <th>Vehicle Type</th>
                <th>Price (thousands)</th>
                <th>Engine Size</th>
                <th>Horsepower</th>
                <th>Wheelbase</th>
                <th>Width</th>
                <th>Length</th>
                <th>Curb Weight</th>
                <th>Fuel Capacity</th>
                <th>Fuel Efficiency</th>
                <th>Latest Launch</th>
                <th>Power Perf Factor</th>
            </tr>
        </thead>
        <tbody>
            {{#each data}}
                <tr>
                    <td>{{replaceEmptyAndHighlight InvoiceNo}}</td>
                    <td>{{replaceEmptyAndHighlight Manufacturer}}</td>
                    <td>{{replaceEmptyAndHighlight class}}</td>
                    <td>{{replaceEmptyAndHighlight Sales_in_thousands}}</td>
                    <td>{{replaceEmptyAndHighlight __year_resale_value}}</td>
                    <td>{{replaceEmptyAndHighlight Vehicle_type}}</td>
                    <td>{{replaceEmptyAndHighlight Price_in_thousands}}</td>
                    <td>{{replaceEmptyAndHighlight Engine_size}}</td>
                    <td>{{replaceEmptyAndHighlight Horsepower}}</td>
                    <td>{{replaceEmptyAndHighlight Wheelbase}}</td>
                    <td>{{replaceEmptyAndHighlight Width}}</td>
                    <td>{{replaceEmptyAndHighlight Length}}</td>
                    <td>{{replaceEmptyAndHighlight Curb_weight}}</td>
                    <td>{{replaceEmptyAndHighlight Fuel_capacity}}</td>
                    <td>{{replaceEmptyAndHighlight Fuel_efficiency}}</td>
                    <td>{{replaceEmptyAndHighlight Latest_Launch}}</td>
                    <td>{{replaceEmptyAndHighlight Power_perf_factor}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</body>
</html>
